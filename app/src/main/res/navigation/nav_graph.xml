<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">
    <fragment
        android:id="@+id/mainFragment"
        android:name="com.rafiqul.huda.jetpacknavigationapp.MainFragment"
        android:label="MainFragment" >
        <action
            android:id="@+id/action_mainFragment_to_inputFragment2"
            app:destination="@id/inputFragment" />
    </fragment>
    <fragment
        android:id="@+id/inputFragment"
        android:name="com.rafiqul.huda.jetpacknavigationapp.InputFragment"
        android:label="InputFragment" >
        <action
            android:id="@+id/action_inputFragment_to_outputFragment"
            app:destination="@+id/outputFragment" />
    </fragment>
    <fragment
        android:id="@+id/outputFragment"
        android:name="com.rafiqul.huda.jetpacknavigationapp.OutputFragment"
        android:label="OutputFragment" />
</navigation>